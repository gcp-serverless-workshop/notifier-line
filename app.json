{
  "name": "notifier-line",
  "env": {
    "LOTIFY_ACCESS_TOKEN": {
      "description": "LINE Notify secret key",
      "value": "ooooooo",
      "required": true
    },
    "API_ENV": {
      "value": "production",
      "required": true
    },
    "LOG": {
      "description": "Log level",
      "value": "INFO"
    },
    "GOOGLE_CLOUD_PROJECT": {
      "description": "Google cloud project name",
      "value": "xxxx"
    }
  },
  "options": {
    "allow-unauthenticated": true,
    "memory": "512Mi",
    "cpu": "1",
    "http2": false,
    "concurrency": 10,
    "max-instances": 5
  }
}
